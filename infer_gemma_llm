from transformers import AutoTokenizer, AutoModelForCausalLM
pt_model_name = "google/gemma-7b"

tokenizer = AutoTokenizer.from_pretrained(pt_model_name)
model = AutoModelForCausalLM.from_pretrained(pt_model_name)

input_text = "What Shakespeare's Hamlet would think about the Artificial Intelligence."
input_ids = tokenizer(input_text, return_tensors="pt")

outputs = model.generate(**input_ids)
print(tokenizer.decode(outputs[0]))
